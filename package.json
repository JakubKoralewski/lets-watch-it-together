{
  "name": "lets-watch-it-together",
  "author": "@JakubKoralewski",
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "dev-seed": "ts-node -r tsconfig-paths/register -P prisma/seeding/tsconfig.script.json prisma/seeding/initiateDb.ts | npx pino-pretty -t",
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev | npx pino-pretty -t -f",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test:docker": "prisma migrate dev --preview-feature && npm run dev-seed && npm run test:all",
    "test:all": "npm run test:e2e:run && npm run test:jest && npm run test:type-check",
    "test:jest": "jest",
    "test:e2e:run": "cypress run",
    "test:e2e:open": "cypress open",
    "test:type-check": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.2.10",
    "@prisma/client": "^2.14.0",
    "@reduxjs/toolkit": "^1.5.0",
    "@types/redis": "^2.8.28",
    "date-fns": "^2.16.1",
    "framer-motion": "^3.1.1",
    "got": "^11.8.1",
    "lodash": "^4.17.20",
    "material-ui-dropzone": "^3.5.0",
    "next": "^10.0.5",
    "next-auth": "^3.2.0-canary.35",
    "next-pwa": "^5.0.1",
    "pino": "^6.10.0",
    "pino-caller": "^3.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.2",
    "redis": "^3.0.2",
    "sharp": "^0.27.0",
    "swr": "^0.4.0"
  },
  "devDependencies": {
    "@prisma/cli": "^2.14.0",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/react": "^10.0.1",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.167",
    "@types/next-auth": "^3.1.18",
    "@types/node": "^13.13.38",
    "@types/node-fetch": "^2.5.8",
    "@types/pino": "^6.3.5",
    "@types/react": "^16.14.2",
    "@types/react-redux": "^7.1.15",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "babel-jest": "^25.2.3",
    "cross-env": "^7.0.3",
    "cypress": "^6.2.1",
    "cypress-social-logins": "^1.9.3",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.3",
    "jest-watch-typeahead": "^0.5.0",
    "lint-staged": "^10.0.10",
    "nanoid": "^3.1.20",
    "node-fetch": "^2.6.1",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.2.1",
    "promptly": "^3.2.0",
    "ts-node": "^9.1.1",
